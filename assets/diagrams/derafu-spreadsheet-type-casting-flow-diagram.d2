direction: down

# Entities
file: File {
  shape: document
  style: {
    fill: "#EEEEEE"
  }
}

loader: Loader {
  shape: rectangle
  style: {
    fill: "#E0F7FA"
    stroke: "#0097A7"
  }
}

handler: Format Handler {
  shape: rectangle
  style: {
    fill: "#FFEBEE"
    stroke: "#C62828"
  }
}

raw: Raw Data {
  shape: cylinder
  style: {
    fill: "#FFE0B2"
    stroke: "#E65100"
  }
}

caster: Type Caster {
  shape: rectangle
  style: {
    fill: "#E8F5E9"
    stroke: "#388E3C"
    bold: true
  }
}

typed: Typed Data {
  shape: cylinder
  style: {
    fill: "#BBDEFB"
    stroke: "#1976D2"
    bold: true
  }
}

# Process Flow
file -> loader
loader -> handler: uses
handler -> raw: extracts
raw -> caster: processes
caster -> typed: converts
typed -> app: "\"123\" -> 123\n\"true\" -> true\n\"2025-03-12\" -> DateTime"

app: Your Application {
  shape: hexagon
  style: {
    fill: "#D1C4E9"
    stroke: "#512DA8"
  }
}
